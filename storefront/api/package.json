{
  "name": "storefront-api",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "engines": {
    "node": ">=16.0.0 <17.0.0"
  },
  "scripts": {
    "start": "node ./dist/server.js",
    "zip-win": "cd dist && 7z a -tzip Archive.zip && cd ..",
    "build-win": "npm run clean && tsc && copy \"./src/config/\\config.production.env\" \"./dist/config/\" && mkdir \"./dist/.elasticbeanstalk/\"  && copy \"./.elasticbeanstalk\" \"./dist/.elasticbeanstalk/\" && copy package.json \"./dist/\" && copy .npmrc \"./dist/\" && copy package.json \"./dist/\" && copy package-lock.json \"./dist/\" && npm run zip",
    "clean-win": "if exist \"./dist\" rmdir /s /q \"./dist\"",
    "build": "npm run clean && tsc && cp -rf src/config dist/config && cp .npmrc dist/.npmrc && cp package.json dist/package.json && cd dist && zip -r Archive.zip . && cd ..",
    "clean": "rm -rf dist/ || true",
    "deploy": "npm run build && eb list && eb use api-postgresql-dev && eb deploy",
    "deploy-env": "aws elasticbeanstalk update-environment --environment-name api-postgresql-dev --option-settings file://.ebextensions\\options.json",
    "dev": "SET NODE_ENV=development && nodemon src/server.ts",
    "prod": "SET NODE_ENV=production && nodemon src/server.ts",
    "esm": "ts-node-esm src/server.ts ",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "SET NODE_ENV=test && npm run build && db-migrate db:create storefront_backend_test && db-migrate --env test up && jasmine && db-migrate db:drop storefront_backend_test",
    "format:check": "prettier --config .prettierrc src/**/*.ts --check",
    "format:fix": "prettier --config .prettierrc src/**/*.ts --write",
    "db-up": "db-migrate up"
  },
  "author": "Abdulrahman Alyahay",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "guid-typescript": "^1.0.9",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "password-validator": "^5.3.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "typescript": "^4.1.3",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.9",
    "@types/inversify": "^2.0.33",
    "@types/jasmine": "^3.10.7",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.12",
    "@types/winston": "^2.4.4",
    "jasmine": "^3.99.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^4.2.9"
  }
}